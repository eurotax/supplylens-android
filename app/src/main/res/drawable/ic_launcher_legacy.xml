<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background Layer -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#0B0F14"/>
            <size android:width="48dp" android:height="48dp"/>
        </shape>
    </item>
    
    <!-- Gold Frame -->
    <item android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp">
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:color="#D4AF37"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>
    
    <!-- S Letter -->
    <item android:left="12dp" android:top="14dp" android:right="26dp" android:bottom="26dp">
        <shape android:shape="rectangle">
            <solid android:color="#D4AF37"/>
        </shape>
    </item>
    
    <!-- L Letter -->
    <item android:left="26dp" android:top="14dp" android:right="12dp" android:bottom="26dp">
        <shape android:shape="rectangle">
            <solid android:color="#D4AF37"/>
        </shape>
    </item>
</layer-list>
